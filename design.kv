					radius:"10dp"
					padding:"10dp"
					line_color:1,0,1,1
					
					#El nombre del rectángulo
					MDLabel:
						text:"JAGUAR CONTROL REMOTO"
						halign:"center"
						pos_hint:{"center_y":0.5}

					#El Swich para cambiar de estilo oscuro a claro y al contrario

					MDSwitch:
						pos_hint:{"center_y":0.5}
						on_active:
							app.change_style(*args)	

			#la Matriz de tarjetas							
			MDGridLayout:
				cols:3
				size_hint:1,.8
				padding:{"10dp","10dp","10dp","10dp"}
				spacing:"10dp"

				#Definicion de las tarjeta personalizable, solo se pone lo que cambia

