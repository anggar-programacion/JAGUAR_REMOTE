#: import RiseInTransition kivy.uix.screenmanager.RiseInTransition
#: import SlideTransition kivy.uix.screenmanager.SlideTransition




#Tarjeta personalizada ElementCard derivada de MDCard
<ElementCard@MDCard>:
	size: "200dp", "100dp"
	radius:"10dp"
	spacing:"10dp"
	padding:"10dp"
	text:""
	sub_text:""
	orientation:"vertical"
	ripple_behavior: True
	on_release:
		app.root.transition= RiseInTransition()
	MDBoxLayout:
		orientation:"vertical"

		#meter aqui una imagen
	
		MDLabel:
			text:root.text
			halign:"center"
			text_color: 0, 0, 1, 1
			

		MDLabel:
			text:root.sub_text
			halign:"center"
			text_color: 1,1,1,1

		

#Definicion de Pantalla personalizada CustomizeScreen derivada de MDBoxLayaut
<CustomizeScreen@MDBoxLayout>:
	orientation:"vertical"
	text:""
	padding:100
	MDLabel:
		text:root.text
		halign:"center"
		pos_hint:{"center_x":.5}
	MDFillRoundFlatButton:
		text:"volver.."
		pos_hint:{"center_x":.5}
		on_release:
			app.root.current="pantalla principal"
			app.root.transition=SlideTransition(direction="left")


		

#Definicion de la pantalla inicial

<ui>:
	MDScreen:
		name: "pantalla principal"
		md_bg_color:0,.5,.5,1

				
		MDBoxLayout:
			orientation: "vertical"

			#El rectangulo de arriba con el nombre de la APP
			
			MDBoxLayout:
				size_hint:1,.2
				orientation:"horizontal"
				padding:"10dp"
